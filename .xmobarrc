Config { font = "xft:inconsolata:size=12:antialias=true:hinting=true"
, bgColor = "#090300"
, fgColor = "#01a252"
, alpha = 100
, position = Top
, lowerOnStart = True
, commands = [ Run Weather "ZSPD" ["-t", "<station>: <tempC>C","-L","18","-H","25","--normal","green","--high","red","--low","lightblue"] 36000
			, Run Network "wlp1s0b1" ["-L","0","-H","32","--normal","green","--high","red"] 10
			, Run Cpu ["-L","3","-H","50","--normal","green","--high","red"] 10
			, Run Battery ["--template", "Battery: <acstatus>"
				, "--Low"	, "10"	-- unit: %
				, "--High"	, "80"	-- units: %
				, "--low"   , "darkred"
				, "--normal", "darkorange"
				, "--high"	, "darkgreen"
				, "--" -- battery specific options
					   -- discharging status
				, "-o"	, "<left>% (<timeleft>)"
						-- AC "on" status
				, "-O"  , "<fc=#daa520>Charging</fc>"
						-- charged status
				, "-i"  , "<fc=#006000>100%</fc>"
			] 50
			, Run Memory ["-t", "Mem: <used>M Used <free>M Free"] 10
			, Run Swap [] 10
			, Run Date "%a %b %_d %Y %H:%M:%S" "date" 10
			]
, sepChar = "%"
, alignSep = "}{"
, template = "%cpu% | %memory% | %wlp1s0b1%}{ %ZSPD% | <fc=#ee9a00>%date%</fc> | %battery%"
}
